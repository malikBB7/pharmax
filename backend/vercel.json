{
    "version": 2, 
    "builds": [
      {
        "src": "api/server.mjs", 
        "use": "@vercel/node"  
      }
    ],
    "routes": [
      {
        "src": "/get-access-token", 
        "dest": "/api/server.mjs"       
      }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
}
  
